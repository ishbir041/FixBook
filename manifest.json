{
  "name": "FixBook",
  "version": "0.0",
  "manifest_version": 1,
  "description": "Provide security nudges and hide malicious content on facebook",
  "browser_action": {
    "default_popup": "popup.html"
  },
    /*
    "content_scripts": [
    {
      "matches": ["http://*.google.com/*", "http://*.facebook.com/*"],
      "js": ["change.js"],
      "all_frames": true
    }],
    */
  "permissions": ["tabs", "<all_urls>"]
}